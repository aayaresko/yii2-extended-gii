{
    "name": "aayaresko/yii2-extended-gii",
    "description": "The Gii extension for the Yii framework with twig support",
    "keywords": ["yii2", "gii", "twig", "code generator"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/aayaresko/yii2-extended-gii/issues",
        "source": "https://github.com/aayaresko/yii2-extended-gii"
    },
    "authors": [
        {
            "name": "Andrey Yaresko",
            "email": "aayaresko@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": ">=2.0.4",
        "yiisoft/yii2-bootstrap": "~2.0",
        "yiisoft/yii2-twig": "*",
        "phpspec/php-diff": ">=1.0.2",
        "bower-asset/typeahead.js": "0.10.* | ~0.11.0"
    },
    "autoload": {
        "psr-4": {
            "aayaresko\\gii\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
